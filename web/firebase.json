{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/privacy",
        "destination": "/privacy.html"
      },
      {
        "source": "/support",
        "destination": "/support.html"
      },
      {
        "source": "/terms",
        "destination": "/terms.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}