<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>ITSAppUsesNonExemptEncryption</key>
		<false/>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>HoopRank</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>HoopRank</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>LSRequiresIPhoneOS</key>
		<true/>
		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>HoopRank needs your location to find nearby courts and players.</string>
		<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
		<string>HoopRank needs your location to find nearby courts and players, even when the app is in the background.</string>
		<key>UIStatusBarHidden</key>
		<false/>
		
		<!-- URL Schemes for Social Login -->
		<key>CFBundleURLTypes</key>
		<array>
			<!-- Google Sign-In URL Scheme -->
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLName</key>
				<string>google</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<!-- Reversed Client ID from GoogleService-Info.plist -->
					<string>com.googleusercontent.apps.655663987178-qgoutu8s8j72hij1p81orau0qst05mo4</string>
				</array>
			</dict>
			<!-- Facebook Login URL Scheme -->
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLName</key>
				<string>facebook</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<!-- Facebook App ID with fb prefix -->
					<string>fb3166274956866749</string>
				</array>
			</dict>
		</array>
		
		<!-- Facebook SDK Configuration -->
		<key>FacebookAppID</key>
		<string>3166274956866749</string>
		<key>FacebookClientToken</key>
		<string>168dd1d311a34d0b17a15c48e616f9f7</string>
		<key>FacebookDisplayName</key>
		<string>HoopRank</string>
		
		<!-- Allow Facebook Login in browser -->
		<key>LSApplicationQueriesSchemes</key>
		<array>
			<string>fbapi</string>
			<string>fb-messenger-share-api</string>
			<string>fbauth2</string>
			<string>fbshareextension</string>
		</array>
		
		<!-- Camera and Photo Library Permissions (for image_picker) -->
		<key>NSCameraUsageDescription</key>
		<string>HoopRank needs camera access to take profile photos and capture court images.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>HoopRank needs photo library access to select profile photos and court images.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>HoopRank needs microphone access for video recording.</string>
		
		<!-- Background Modes for Push Notifications -->
		<key>UIBackgroundModes</key>
		<array>
			<string>fetch</string>
			<string>remote-notification</string>
		</array>
	</dict>
</plist>
