{
  "name": "HoopRank Dev (Codespaces)",
  "image": "ghcr.io/cirruslabs/flutter:3.22.0",
  "features": {
    "ghcr.io/devcontainers/features/node:1": { "version": "20" }
  },
  "forwardPorts": [3000, 5432, 6379, 5672, 15672],
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter",
        "ms-azuretools.vscode-docker",
        "dbaeumer.vscode-eslint"
      ]
    }
  },
  "postCreateCommand": "flutter doctor -v && node -v"
}
